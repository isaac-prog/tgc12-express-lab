{{#extends 'base'}}

{{#block 'content'}}
<h1>Adding new note for {{food.foodName}}</h1>
<form method="POST">
    <div>
        <label class="form-label">Notes:</label>
        <textarea class="form-control" name="content"></textarea>
    </div>
    <input type="submit" class="btn btn-primary mt-3"/>
</form>
{{/block}}

{{/extends}}